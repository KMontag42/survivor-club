<%= form_for @contestant, :layout => :basic do |f| %>
  <%= f.text_field :name %>
  <%= f.number_field :age %>
  <%= f.text_field :job %>
  <%= f.text_field :hometown %>
  <%= f.select :seasons, Season.all.collect {|p| [ p.name, p.id ] },
               {},
               { :multiple => true } %>
  <%= f.submit 'Update' %>
<% end %>
